    "description": "SDK NetForce Email Saude.",
    "keywords"   : ["netforce", "email", "saude", "sdk-php"],
    "license"    : "GPL-3",
    "type"       : "project",

    "authors": [
        {
            "name" : "Bruno Goncalves",
            "email": "bugotech@gmali.com"
        }
    ],

    "require": {
        "php"               : ">=5.5.0",
        "nano7/SDK-PHP"     : "dev-master"
    },

    "autoload": {
